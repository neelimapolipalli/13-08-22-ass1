#include <stdio.h>

int main()
{
    int i,sum=0,tot,ttl,res,max,n;
    int a[20];
int len;
printf("Enter size of array");
scanf("%d",&len);


    for(i=0;i<len;i++)
    {
        scanf("%d",&a[i]);
    
        sum=sum+a[i];
}
max=len-1;
n=len+1;
//printf("%d\n",a[max]);
ttl=(a[0]+a[max]);
tot=(ttl)*n/2;
res=tot-sum;
//printf("total is %d\n",tot);
printf(" Missing number is%d\n",res);
    return 0;
}
